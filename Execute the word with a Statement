#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
     char input[100];
    char firstWord[100];
    fgets(input, sizeof(input), stdin);
    sscanf(input, "%s", firstWord);
    printf("May I know how to learn %s!!!...\n", firstWord);
    return 0;
}
