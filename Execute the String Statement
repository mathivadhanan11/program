#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char name[100];
    
    fgets(name, sizeof(name), stdin);
    
    int i = 0;
    while (name[i] != '\0') {
        if (name[i] == '\n') {
            name[i] = '\0';
            break;
        }
        i++;
    }
    
    printf("Hai %s!\tWelcome to Programming Language...\n", name);
    return 0;
}
